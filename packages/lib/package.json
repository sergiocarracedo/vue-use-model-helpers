{
  "name": "vue-use-model-helpers",
  "version": "4.0.0-beta.2",
  "description": "Helpers to manage custom v-model and .sync using composition API",
  "author": "Sergio Carracedo <hi@sergiocarracedo.es>",
  "license": "MIT",
  "keywords": [
    "vue",
    "vue3",
    "v-model",
    "composition-api",
    "props",
    "Avoid mutating a prop directly"
  ],
  "workspaces": [
    "lib",
    "demo-vue2",
    "demo-vue3"
  ],
  "module": "dist/vue-use-model-helpers.esm.js",
  "unpkg": "dist/vue-use-model-helpers.global.js",
  "main": "dist/vue-use-model-helpers.cjs.js",
  "typings": "dist/types/index.d.ts",
  "scripts": {
    "build": "rollup -c",
    "build:watch": "rollup -c -w",
    "test": "jest",
    "switch:2": "vue-demi-switch 2 vue2",
    "switch:3": "vue-demi-switch 3",
    "publish": "rollup -c & npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sergiocarracedo/vue-use-model-helpers.git"
  },
  "bugs": {
    "url": "https://github.com/sergiocarracedo/vue-use-model-helpers/issues"
  },
  "homepage": "https://github.com/sergiocarracedo/vue-use-model-helpers#readme",
  "peerDependencies": {
    "vue": "^2.7.0 || >=3.0.0"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  },
  "devDependencies": {
    "@babel/core": "^7.19.6",
    "@babel/types": "^7.19.4",
    "@rollup/plugin-babel": "6.0.2",
    "@rollup/plugin-typescript": "^8.2.1",
    "@types/jest": "^25.2.1",
    "@vue/compiler-sfc": "^3.0.11",
    "rollup": "^2.43.0",
    "tslib": "^2.4.0",
    "typescript": "^4.2.4",
    "vue": "^3.2.x",
    "vue2": "npm:vue@2.7"
  },
  "dependencies": {
    "vue-demi": "^0.13.11"
  }
}
